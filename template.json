{
  "name": "statenweb",
  "parse": [
    "Capfile",
    "bower.json",
    "composer.json",
    "Gruntfile.js",
    "package.json",
    "config/deploy.rb",
    "config/deploy/staging.rb",
    "config/deploy/production.rb"
  ],
  "discard": [
    "filters",
    "template.json",
    "inquirer-questions.json"
  ],
  "move": [
    { "web/app/themes/template-theme": "web/app/themes/{{ name }}" }
  ],
  "filters": {
    "markdown": "filters/markdown.js"
  },
  "questions": "inquirer-questions.js"
}
